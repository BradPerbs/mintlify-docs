{
    "$schema": "https://mintlify.com/schema.json",
    "name": "Geogen",
    "theme": "mint",
    "logo": {
        "dark": "/logo/dark.png",
        "light": "/logo/light.png"
    },
    "favicon": "/favicon.png",
    "colors": {
        "primary": "#4D4D4D",
        "light": "#808080",
        "dark": "#000000"
    },
    "openapi": [
        "openapi.json"
    ],
    "api": {
        "baseUrl": "https://api.geogen.io/v1",
        "auth": {
            "method": "bearer"
        },
        "playground": {
            "mode": "show"
        }
    },
    "navigation": {
        "tabs": [
            {
                "tab": "Documentation",
                "icon": "book-blank",
                "global": {
                    "anchors": [
                        {
                            "anchor": "Blog",
                            "icon": "newspaper",
                            "color": {
                                "light": "#070707",
                                "dark": "#070707"
                            },
                            "href": "https://www.geogen.io/articles"
                        },
                        {
                            "anchor": "Changelog",
                            "icon": "layer-group",
                            "color": {
                                "light": "#070707",
                                "dark": "#070707"
                            },
                            "href": "https://www.geogen.io/changelog"
                        }
                    ]
                },
                "groups": [
                    {
                        "group": "General",
                        "pages": [
                            "index",
                            "quick-start/quickstart",
                            "quick-start/prompts-configuration",
                            "quick-start/competitors-selection"
                        ]
                    }
                ]
            },
            {
                "tab": "Public APIs",
                "icon": "code",
                "groups": [
                    {
                        "group": "Overview",
                        "pages": [
                            "api-reference/introduction",
                            "api-reference/authentication",
                            "api-reference/rate-limits"
                        ]
                    },
                    {
                        "group": "Workspace",
                        "pages": [
                            {
                                "openapi": "openapi.json GET /workspace"
                            },
                            {
                                "openapi": "openapi.json GET /workspace/members"
                            },
                            {
                                "openapi": "openapi.json GET /workspace/tags"
                            }
                        ]
                    },
                    {
                        "group": "Entities",
                        "pages": [
                            {
                                "openapi": "openapi.json GET /entities"
                            },
                            {
                                "openapi": "openapi.json POST /entities"
                            },
                            {
                                "openapi": "openapi.json GET /entities/prompts"
                            },
                            {
                                "openapi": "openapi.json POST /entities/addprompt"
                            },
                            {
                                "openapi": "openapi.json DELETE /entities/deleteprompt"
                            },
                            {
                                "openapi": "openapi.json GET /entities/citations"
                            }
                        ]
                    },
                    {
                        "group": "Analytics",
                        "pages": [
                            {
                                "openapi": "openapi.json GET /competitors"
                            },
                            {
                                "openapi": "openapi.json GET /responses"
                            },
                            {
                                "openapi": "openapi.json GET /trends/visibility"
                            },
                            {
                                "openapi": "openapi.json GET /trends/sentiment"
                            }
                        ]
                    },
                    {
                        "group": "Models",
                        "pages": [
                            {
                                "openapi": "openapi.json GET /models"
                            }
                        ]
                    }
                ]
            }
        ]
    },
    "footerSocials": {
        "linkedin": "https://www.linkedin.com/company/geogenai/",
        "twitter": "https://x.com/Geogenai",
        "facebook": "https://www.facebook.com/geogenai/",
        "instagram": "https://www.instagram.com/geogen.io/"
    }
}